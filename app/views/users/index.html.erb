<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<table>
	<tr>
		<td>id</td>
		<td>name</td>
		<td>email</td>
		<td>pass</td>
	</tr>
	<% @allAccounts.each do |account| %>
	<tr>
		<td><%= account.id %></td>
		<td><%= account.name %></td>
		<td><%= account.email %></td>
		<td><%= account.password_digest %></td>
	</tr>
	<% end %>
</table>
